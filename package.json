{
  "name": "react-codescreen-assessment",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "react-scripts test",
    "cy:open": "cypress open",
    "cy:test": "cy:open & cypress run",
    "start": "react-scripts start",
    "build": "tsc",
    "lint": "tsc --project tsconfig.lint.json",
